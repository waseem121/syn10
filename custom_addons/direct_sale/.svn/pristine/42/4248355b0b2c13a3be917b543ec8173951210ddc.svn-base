<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="view_company_form_acc_psports_inherit1" model="ir.ui.view">
            <field name="name">res.company.form.acc.psports.inherit1</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form" />
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page string="Report Configuration">
                        <group>
                            <field name="header_image_qweb" widget="image" />
                            <field name="footer_image_qweb" widget="image" />
                        </group>
                    </page>
                </xpath>
            </field>
        </record>

    </data>
</odoo>