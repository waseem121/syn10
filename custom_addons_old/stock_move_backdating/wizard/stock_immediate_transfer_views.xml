<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_stock_immediate_transfer_inherit_form" model="ir.ui.view">
        <field name="name">stock.immediate.transfer.backdating.form</field>
        <field name="model">stock.immediate.transfer</field>
        <field name="inherit_id" ref="stock.view_immediate_transfer" />
        <field name="arch" type="xml">
            <xpath expr="//group" position="after">
                <group >
                    <field name="date_backdating" colspan="2"/>
                </group>
            </xpath>
        </field>
    </record>
    
</odoo>